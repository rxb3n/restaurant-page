{"ast":null,"code":"import NavBar from './components/NavBar.vue';\nimport HeroView from './components/HeroView.vue';\nimport FooterView from './components/FooterView.vue';\nimport GridView from \"./components/GridView.vue\";\nimport menuView from \"./components/menu.vue\";\nexport default {\n  name: 'App',\n  components: {\n    NavBar,\n    HeroView,\n    FooterView,\n    GridView\n  }\n};","map":{"version":3,"names":["NavBar","HeroView","FooterView","GridView","menuView","name","components"],"sources":["C:\\Users\\israel\\Desktop\\restaurant-page\\src\\App.vue"],"sourcesContent":["<template>\r\n  <!-- NAVBAR AND HERO SECTION -->\r\n  <NavBar/>\r\n  <HeroView id=\"section-0\"></HeroView>\r\n\r\n<!-- VIDEO SECTION -->\r\n\r\n<section id=\"section-1\">\r\n  <div class=\"video-container\">\r\n    <video class=\"video-main\" autoplay muted playsinline loop>\r\n      <source src=\"./assets/video-main.mp4\" type=\"video/mp4\">\r\n    </video>\r\n  </div>\r\n</section>\r\n\r\n<!-- MENU SECTION -->\r\n\r\n<section class=\"menu-container\" id=\"section-2\">\r\n\r\n  <div class=\"menu-box\">\r\n\r\n    <div class=\"sauces\">\r\n\r\n      <h2 class=\"text-primary\" >NOS SAUCES (3000 CFA)</h2>\r\n    <div class=\"menu-item\" >\r\n      <p>Sauce Gombo (Kopè)</p>\r\n      <p id=\"text-secondary\">Sauce gluante à base de gombo frais ou secs.</p>\r\n    </div>\r\n    <div class=\"menu-item\" >\r\n      <p>Sauce Arachide</p>\r\n      <p id=\"text-secondary\">Sauce onctueuse et parfumée à base de beurre d’arachide, de lait de coco, <br> de sauce hoisin et de sambal oelek.</p>\r\n    </div>\r\n    <div class=\"menu-item\" >\r\n      <p>Sauce Graine</p>\r\n      <p id=\"text-secondary\">Une sauce onctueuse et parfumée à base de noix de palme, <br> mijotée avec des morceaux de queue de bœuf, des crabes, des tripes et du poisson frais.</p>\r\n    </div>\r\n    <div class=\"menu-item\" >\r\n      <p>Sauce Feuille</p>\r\n      <p id=\"text-secondary\">A base de feuilles de manioc, gombo, patates ou épinards. <br> Mijotée avec de la viande ou du poisson fumé, des oignons, des tomates et des épices. <br> Idéale pour accompagner du riz ou du foutou</p>\r\n    </div>\r\n    <div class=\"menu-item\" >\r\n      <p>Sauce Tomate Cocktail</p>\r\n      <p id=\"text-secondary\"> Une sauce crémeuse et piquante à base de mayonnaise, de sauce tomate, de Tabasco et de cognac.</p>\r\n    </div>\r\n    <div class=\"menu-item\" >\r\n      <p>Sauce Gouagouassou</p>\r\n      <p id=\"text-secondary\"> Une sauce onctueuse et parfumée à base de deux sauces mélangées: <br> une sauce aux gombos et une sauce à l’aubergine..</p>\r\n    </div>\r\n\r\n    <div class=\"btn-menu-container\">\r\n      <a class=\"btn btn-menu\">VOIR MENU COMPLET</a>\r\n    </div>\r\n\r\n    </div>\r\n\r\n    <div class=\"accomp-container\">\r\n\r\n<div class=\"accomp-box\">\r\n  <h3 class=\"text-primary\">ACCOMPAGNEMENTS (1000 CFA)</h3>\r\n  <div class=\"accomp-items\">\r\n  <p>Foutou</p>\r\n  <p>Foufou</p>\r\n  <p>Placali</p>\r\n  <p>Akassa</p>\r\n  <p>Kabato</p>\r\n</div>\r\n\r\n</div>\r\n</div>\r\n\r\n\r\n    \r\n\r\n\r\n  </div>\r\n\r\n\r\n\r\n\r\n\r\n  <div class=\"menu-img\">\r\n  </div>\r\n\r\n</section>\r\n\r\n<section class=\"action-container\"  id=\"section-3\">\r\n  <div class=\"action-text\">\r\n    <h2 class=\"text-primary\" >REJOIGNEZ-NOUS</h2>\r\n    <p>\r\n      Lorem, ipsum dolor sit amet consectetur adipisicing elit. \r\n      Dolor ab sapiente aut fuga quae qui iusto dolorem consectetur natus atque.\r\n    </p>\r\n    <a href=\"#about\" class=\"btn-action btn\">Contactez-nous</a>\r\n  </div>\r\n</section>\r\n\r\n\r\n\r\n<!-- CAROUSEL IMAGES SECTION -->\r\n\r\n<section  id=\"#section-4\">\r\n  <GridView/>\r\n</section>\r\n\r\n<!-- FOOTER -->\r\n\r\n<FooterView/>\r\n\r\n</template>\r\n\r\n<script>\r\nimport NavBar from './components/NavBar.vue';\r\nimport HeroView from './components/HeroView.vue';\r\nimport FooterView from './components/FooterView.vue';\r\nimport GridView from \"./components/GridView.vue\";\r\nimport menuView from \"./components/menu.vue\";\r\n\r\nexport default {\r\n  name: 'App',\r\n  components: {\r\n    NavBar,\r\n    HeroView,\r\n    FooterView,\r\n    GridView,\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style>\r\n#text-secondary {\r\n  font-size: 0.75em;\r\n  color: grey;\r\n}\r\n.bold {\r\n  font-size: 2em;\r\n  font-weight: 800;\r\n}\r\n\r\n.btn {\r\n  background-color: white;\r\n  margin-top: 2%;\r\n  border: 3px solid black;\r\n  padding: 10px;\r\n  transition: 0.2s;\r\n}\r\n\r\n.btn-menu {\r\n\r\n}\r\n\r\n.btn-menu-container {\r\n  margin-top: 10%;\r\n  margin-left: 50%;\r\n}\r\n\r\n\r\n.btn:hover {\r\n  background-color: black;\r\n  color: white;\r\n}\r\n\r\n/* ABOUT US SECTION */\r\n.action-container {\r\n  padding-top: 2%;\r\n  padding-bottom: 2%;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  text-align: center;\r\n}\r\n\r\n.btn-action {\r\n  position: relative;\r\n}\r\n\r\n/* MENU SECTION */\r\n\r\n.menu-container {\r\n  background: linear-gradient(\r\n          rgba(0, 0, 0, 0.2), \r\n          rgba(0, 0, 0, 0.4)\r\n        ), url(./assets/thumbnail_image003.jpg);\r\n  display: flex;\r\n  justify-content: center;\r\n  height: 1250px;\r\n\r\n}\r\n\r\n.menu-box {\r\n  background-color: white;\r\n  height: fit-content;\r\n  position: relative;\r\n  top: 20%;\r\n  padding: 3%;\r\n  display: flex;\r\n  text-align: center;\r\n  flex-direction: row;\r\n}\r\n\r\n.menu-img {\r\n  position: relative;\r\n  top: 20%;\r\n}\r\n\r\na{\r\n  color: black;\r\n  text-decoration: none;\r\n}\r\n\r\n/* ACCOMPAGNEMENTS */\r\n.accomp-container {\r\n  height: 500px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  margin-bottom: 1%;\r\n}\r\n\r\n.accomp-box {\r\n  position: relative;\r\n  background-color: white;\r\n  color: black;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  padding: 2%;\r\n}\r\n\r\n</style>\r\n"],"mappings":"AA+GA,OAAOA,MAAK,